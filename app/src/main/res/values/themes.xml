<resources>
    <!-- Base application theme. -->
    <style name="Theme.Web_browser" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/teal_dark</item>
        <item name="colorPrimaryVariant">@color/teal_light_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_dark</item>
        <item name="colorSecondaryVariant">@color/green_light_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#80000000</item>
        <!-- Customize your theme here. -->
    </style>
</resources>