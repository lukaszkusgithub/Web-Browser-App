<resources >
    <!-- Base application theme. -->
    <style name="Theme.Web_browser" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_dark_night</item>
        <item name="colorPrimaryVariant">@color/primary_light_night</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primary_light_night</item>
        <item name="colorSecondaryVariant">@color/accent_dark_night</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#80000000</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:tint">@color/white</item>

        <!-- Customize your theme here. -->
    </style>
</resources>