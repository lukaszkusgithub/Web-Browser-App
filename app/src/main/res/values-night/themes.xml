<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Web_browser" parent="Theme.MaterialComponents.DayNight.NoActionBar"
        >
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_dark_night</item>
        <item name="colorPrimaryVariant">@color/primary_light_night</item>
        <item name="colorOnPrimary">@color/primary_dark_night</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/white</item>
        <item name="colorSecondaryVariant">@color/accent_dark_night</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/accent_dark_night</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:textColor">@color/white</item>
        <!-- Customize your theme here. -->


    </style>
</resources>